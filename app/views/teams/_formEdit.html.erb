<%= form_with url: organization_team_path(1,team), model: team do |form| %>
    <div class="mb-3 ml-3">
      <%= form.label :name %><br>
      <%= form.text_field :name %>
    <%= form.submit "Submit"  , class:"btn btn-sm btn-dark" %>
    </div>
  <% end %> 

<%= form_with url: organization_user_path(1), method: :patch  do |form| %>
   <div class="ml-3">
      <label class="form-label" for="form3Example1c">Available</label></br>
      <%= form.select :emp, @emps.map {|emp| [emp.email, emp.id] } %> 
      <%= form.hidden_field :team_id, value: @team.id %>
      <%= form.submit "Add Member", class:"btn btn-sm btn-dark" %>
  </div>
<% end %>


  